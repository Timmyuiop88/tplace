{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/product/:id",
      "destination": "/product/:id"
    },
    {
      "source": "/vendor/products/edit/:id",
      "destination": "/vendor/products/edit/:id"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
